{
  "name": "bolt-test",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:icons": "tsx ./scripts/build-icons.ts",
    "build:react-router": "react-router build",
    "predev": "npm run build:icons --silent",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "lint": "eslint --fix .",
    "stylelint": "stylelint --fix 'app/**/*.css'",
    "format": "prettier --write .",
    "typegen": "react-router typegen",
    "typecheck": "tsc",
    "i18n:resources": "i18next-resources-for-ts interface -i ./app/modules/i18n/locales/en -o ./app/modules/i18n/types/i18next.resources.d.ts && prettier --write ./app/modules/i18n/types/i18next.resources.d.ts"
  },
  "dependencies": {
    "@epic-web/client-hints": "^1.3.5",
    "@epic-web/invariant": "^1.0.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "isbot": "^5.1.23",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "remix-i18next": "^7.0.2"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.26.2",
    "@eslint/js": "^9.20.0",
    "@react-router/dev": "^7.2.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "browserlist": "^1.0.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "i18next-resources-for-ts": "^1.5.0",
    "lightningcss": "^1.29.1",
    "node-html-parser": "^7.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.1",
    "stylelint": "^16.14.1",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^37.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vite-plugin-run": "^0.6.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
